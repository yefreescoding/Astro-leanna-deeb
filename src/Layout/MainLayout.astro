---
// import Global Styles Folder
import "../styles/global.css";

// import Layout
import MainHeader from "./MainHeader.astro";

import MainFooter from "./MainFooter.astro";

// import components
import Head from "../Components/Head.astro";

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
    <>
        <Head {title} />
        <MainHeader />
        <body>
            <slot />
            <MainFooter />
        </body>
    </>
</html>
